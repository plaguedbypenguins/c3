if ( $?PATH == 0 ) then
    setenv PATH "@prefix@/usr/bin"
else
    setenv PATH "${PATH}:@prefix@/usr/bin"
endif

if ( $?MANPATH == 0 ) then
    setenv MANPATH "@prefix@/doc/man"
else
    setenv MANPATH "${MANPATH}:@prefix@/doc/man/"
endif

